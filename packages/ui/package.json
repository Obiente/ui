{
  "name": "@obiente/ui",
  "version": "0.1.0",
  "description": "Vue 3 UI components with extensive theming support",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "development": "./src/index.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./styles": {
      "development": "./src/styles/index.css",
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    },
    "./utils": {
      "types": "./dist/index.d.ts",
      "development": "./src/index.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./composables": {
      "types": "./dist/index.d.ts",
      "development": "./src/index.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "src/styles"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@ark-ui/vue": "^5.22.0",
    "@obiente/themes": "workspace:*",
    "@tailwindcss/vite": "^4.1.13",
    "tailwindcss": "^4.1.13",
    "vue": "^3.5.21"
  },
  "devDependencies": {
    "@types/node": "^24.3.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "typescript": "^5.9.2",
    "vite": "^7.1.4",
    "vite-plugin-dts": "^4.5.4"
  },
  "keywords": [
    "vue",
    "ui",
    "components",
    "theming",
    "design-system"
  ],
  "license": "AGPL-3.0-only"
}
